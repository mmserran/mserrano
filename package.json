{
  "name": "LAB-MSERRANO",
  "version": "1.0.0",
  "description": "to the lab",
  "main": "index.js",
  "repository": "https://github.com/mserrano-dev/LAB-MSERRANO.git",
  "author": "ENTslug",
  "license": "Apache-2.0",
  "dependencies": {
    "angular": "1.6.5",
    "angular-mocks": "1.6.5",
    "angular-route": "1.6.5"
  },
  "scripts": {
    "preinstall": "(yarn global list | grep -q 'webpack') && : || yarn global add webpack",
    "dev": "webpack --mode development --watch",
    "prod": "webpack --mode production",
    "test": "yarn karma start frontend/tests/coverage.conf.js && php test_coverage.php unittests/ --silent && google-chrome tmp/index.html",
    "ut:cov": "yarn karma start frontend/tests/coverage.conf.js && php test_coverage.php unittests/ --silent && google-chrome tmp/index.html",
    "ut:cov:js": "yarn karma start frontend/tests/coverage.conf.js && google-chrome tmp/js-coverage-report/index.html",
    "ut:cov:php": "php test_coverage.php unittests/ --silent && google-chrome tmp/php-coverage-report/index.html",
    "ut:js": "yarn karma start frontend/tests/karma.conf.js",
    "ut:php": "yarn watch 'php test_runner.php unittests/'",
    "?": "webpack --verbose"
  },
  "devDependencies": {
    "css-loader": "0.28.11",
    "extract-text-webpack-plugin": "4.0.0-beta.0",
    "html-loader": "0.5.5",
    "jasmine": "2.9.0",
    "karma": "2.0.0",
    "karma-chrome-launcher": "2.2.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.1",
    "karma-phantomjs-launcher": "1.0.4",
    "node-sass": "4.8.3",
    "sass-loader": "7.0.1",
    "style-loader": "0.20.3",
    "watch": "1.0.2",
    "webpack": "4.6.0",
    "webpack-cli": "2.0.14"
  }
}
